{
  "compilerOptions": {
    "target": "es2019",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noImplicitAny": true,
    "declaration": true,
    "strict": false,
    "esModuleInterop": true,
    "baseUrl": ".",
    "outDir": "./dist",
    "paths": {
      "@/src": [
        "./src"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "tsc-alias": {
    "verbose": false,
    "resolveFullPaths": true
  }
}